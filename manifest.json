{
  "manifest_version": 3,
  "name": "Google Result Clicker",
  "version": "1.1",
  "description": "Searches a target site on Google and repeatedly clicks through to it.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
